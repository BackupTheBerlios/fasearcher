<?xml version="1.0" encoding="UTF-8"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>FASearcherServer</groupId>
  <artifactId>FASearcherServer</artifactId>
  <version>0.0.1</version>
  <build>
    <sourceDirectory>./src/main/java</sourceDirectory>
    <finalName>FASearcherServer</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <jboss>
            <version>4</version>
          </jboss>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
          <includes>
            <include>es/si/FASearcherServer/data/*.java</include>
            <include>es/si/FASearcherServer/ejb/*.java</include>
          </includes>
        </configuration>
      </plugin>
    </plugins>
  </build>
    <repositories>
     <repository>
        <id>central</id>
        <name>JBoss Repository</name>
        <url>http://repository.jboss.com/maven2</url>
        <layout>default</layout>
     </repository>
  </repositories>
  
  <dependencies>
    <dependency>
      <groupId>jboss</groupId>
      <artifactId>jboss-jaxrpc</artifactId>
      <version>5.0.0.Beta4</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>jboss</groupId>
      <artifactId>jboss-saaj</artifactId>
      <version>5.0.0.Beta4</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.1</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>jboss</groupId>
      <artifactId>jboss-javaee</artifactId>
      <version>5.0.0.Beta4</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>jboss</groupId>
      <artifactId>jbossws-spi</artifactId>
      <version>5.0.0.Beta4</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>jboss</groupId>
      <artifactId>jbossws-core</artifactId>
      <version>5.0.0.Beta4</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>jboss</groupId>
      <artifactId>jboss-jaxws</artifactId>
      <version>5.0.0.Beta4</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>jboss</groupId>
      <artifactId>jboss-ejb3x</artifactId>
      <version>5.0.0.Beta4</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>es.si</groupId>
      <artifactId>ProgramadorGenetico</artifactId>
      <version>0.1</version>
    </dependency>
  </dependencies>
</project>